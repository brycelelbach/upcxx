
/* define if UPC++ multidimensional array is enabled */
#undef UPCXX_HAVE_MD_ARRAY

/* define if C++11 features are enabled */
#undef UPCXX_HAVE_CXX11

/* define if C++14 features are enabled */
#undef UPCXX_HAVE_CXX14

/* define if libcxx implements std::is_trivially_destructible */
#undef UPCXX_HAVE_TRIVIALLY_DESTRUCTIBLE

/* define if libcxx implements std::initializer_list */
